          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Server=DESKTOP-3JCV0QN;Database=DiagnosticManagementSystem;Trusted_Connection=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
